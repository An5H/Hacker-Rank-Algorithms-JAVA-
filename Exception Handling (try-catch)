import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ob = new Scanner(System.in);
        try{
            int x = ob.nextInt();
            int y = ob.nextInt();
            System.out.println(x/y);
        }catch(InputMismatchException e){
            System.out.println(e.getClass().getName());
        }
        catch(Exception e){
            System.out.println(e);
        }
    }
}
